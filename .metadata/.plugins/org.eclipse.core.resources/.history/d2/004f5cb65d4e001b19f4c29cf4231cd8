package ABC;

import java.util.Scanner;

public class B {

	public static void s() {
		C c = new C();
		Scanner sc = new Scanner(System.in);
		System.out.println("글자를 입력해주세요");
		c.str = sc.nextLine();

	public static void n() {
		C c = new C();
		Scanner sc = new Scanner(System.in);

		System.out.println("숫자를 입력해주세요");
		c.number = sc.nextInt();
	}

}
