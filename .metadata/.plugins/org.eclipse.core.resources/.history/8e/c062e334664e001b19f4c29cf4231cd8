package ABC;

import java.util.Scanner;

public class C {
	static String str;
	static int number;
	

	public static void str() {
		Scanner sc = new Scanner(System.in);
		System.out.println("글자를 입력해주세요");
		str = sc.nextLine();
	}

	public static void number() {
		Scanner sc = new Scanner(System.in);
		System.out.println("숫자를 입력해주세요");
		number = sc.nextInt();
	}

}
