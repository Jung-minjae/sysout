		int choice = sc.nextInt();

		switch (choice) {
		case 1:
			System.out.println("입력");

			break;
		case 2:
			System.out.println("조회");

			break;

		case 3:
			System.out.println("사용종료하겠습니다");
			run = !run;
			break;
		}

	}

